{
    "manifest_version": 3,
    "name": "Good-Site.org",
    "version": "0.1",
    "description": "Summarizes privacy policy and terms of service of websites.",
    "background": {
      "service_worker": "background.js"
      },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "Privacy.png"
    },
    "permissions": [
    "identity",
    "storage",
    "offscreen"
    ],
    "host_permissions": [
    "https://good-site.org/*",
    "https://*/"
    ]

}