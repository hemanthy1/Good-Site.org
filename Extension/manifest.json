{
    "manifest_version": 2,
    "name": "Good-Site.org",
    "version": "0.1",
    "description": "Summarizes privacy policy and terms of service of websites.",
    "icons": {
      "16": "Privacy.png",
      "48": "Privacy.png",
      "128": "Privacy.png"
    },
    "permissions": [
      "activeTab",
      "storage",
      "https://www.good-site.org/*",
      "identity"
    ],
    "oauth2": {
  "client_id": "sbaw0m4a3m8crjsdcd",
  "scopes": ["user.info.basic"]
},
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
  }